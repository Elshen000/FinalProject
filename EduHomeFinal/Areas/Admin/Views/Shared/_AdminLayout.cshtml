<!DOCTYPE html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>IT Brains Academy Admin</title>
    <!-- plugins:css -->
    <link rel="stylesheet" href="~/admin/vendors/mdi/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="~/admin/vendors/base/vendor.bundle.base.css">
    <!-- endinject -->
    <!-- plugin css for this page -->
    <link rel="stylesheet" href="~/admin/vendors/datatables.net-bs4/dataTables.bootstrap4.css">
    <!-- End plugin css for this page -->
    <!-- inject:css -->
    <link rel="stylesheet" href="~/admin/css/style.css">
    <!-- endinject -->
</head>
<body>
    <div class="container-scroller">
        <!-- partial:partials/_navbar.html -->
        <nav class="navbar col-lg-12 col-12 p-0 fixed-top d-flex flex-row">
            <div class="navbar-brand-wrapper d-flex justify-content-center">
                <div class="navbar-brand-inner-wrapper d-flex justify-content-between align-items-center w-100">
                    <!--************************************* Deyiw *******************************************-->
                    <a class="navbar-brand brand-logo" asp-controller="Dashboard" asp-action="Index"><img style="object-fit:contain" src="~/admin/images/footer-logo (1).png" alt="logo" /></a>
                    <a class="navbar-brand brand-logo-mini" asp-controller="Dashboard" asp-action="Index">IT Brains Academy Admin</a>
                    <button class="navbar-toggler navbar-toggler align-self-center" type="button" data-toggle="minimize">
                        <span class="mdi mdi-sort-variant"></span>
                    </button>
                </div>
            </div>
            <div class="navbar-menu-wrapper d-flex align-items-center justify-content-end">
               
                <ul class="navbar-nav navbar-nav-right">
                   
                  
                    <li class="nav-item nav-profile dropdown">
                        <a>
                            <span class="nav-profile-name">Elşən Quliyev</span>
                        </a>
                       
                    </li>
                </ul>
                <button class="navbar-toggler navbar-toggler-right d-lg-none align-self-center" type="button" data-toggle="offcanvas">
                    <span class="mdi mdi-menu"></span>
                </button>
            </div>
        </nav>
        <!-- partial -->
        <div class="container-fluid page-body-wrapper">
            <!-- partial:partials/_sidebar.html -->
            <nav class="sidebar sidebar-offcanvas" id="sidebar">
                <ul class="nav">
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Dashboard" asp-action="Index">
                            <i class="mdi mdi-home menu-icon"></i>
                            <span class="menu-title">Ana Səhifə</span>
                        </a>
                    </li>


                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Slider" asp-action="Index">
                            <i class="mdi mdi-image-filter menu-icon"></i>
                            <span class="menu-title">Slayder</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="About" asp-action="Index">
                            <i class="mdi mdi-format-align-justify menu-icon"></i>
                            <span class="menu-title"> Haqqında </span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Teacher" asp-action="Index">
                            <i class="mdi mdi mdi-account menu-icon"></i>
                            <span class="menu-title">Müəllimlər</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Event" asp-action="Index">
                            <i class="mdi mdi-chart-pie menu-icon"></i>
                            <span class="menu-title">Tədbirlər</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Speaker" asp-action="Index">
                            <i class="mdi  mdi mdi-account menu-icon"></i>
                            <span class="menu-title">Spikerlər</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Blog" asp-action="Index">
                            <i class="mdi mdi-blogger menu-icon"></i>
                            <span class="menu-title">Bloqlar</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Course" asp-action="Index">
                            <i class="mdi mdi-school menu-icon"></i>
                            <span class="menu-title">Kurslar</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="NoticeBoard" asp-action="Index">
                            <i class="mdi mdi-format-align-justify menu-icon"></i>
                            <span class="menu-title">Elan Lövhəsi</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Users" asp-action="Index">
                            <i class="mdi mdi-account-alert menu-icon"></i>
                            <span class="menu-title">İstifadəçilər</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="Category" asp-action="Index">
                            <i class="mdi mdi-grid menu-icon"></i>
                            <span class="menu-title">Kateqoriyalar</span>
                        </a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-controller="ModeratorCourse" asp-action="Index">
                            <i class="mdi mdi-school menu-icon"></i>
                            <span class="menu-title">Moderatorun Kursları</span>
                        </a>
                    </li>

                </ul>
            </nav>
            <!-- partial -->
            <div class="main-panel">
                <div class="content-wrapper">
                    @RenderBody()
                </div>
                <!-- content-wrapper ends -->
                <!-- partial:partials/_footer.html -->
                <footer class="footer">
                    <div class="d-sm-flex justify-content-center justify-content-sm-between">
                        <p class="text-left">Copyright © <a  target="_blank">Elshen Guliyev</a> 2022. All Right Reserved By Elshen Guliyev.</p>

                    </div>
                    
                </footer>
                <!-- partial -->
            </div>
            <!-- main-panel ends -->
        </div>
        <!-- page-body-wrapper ends -->
    </div>
    <!-- container-scroller -->
    <!-- plugins:js -->

    <script src="~/admin/vendors/base/vendor.bundle.base.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <script src="~/admin/vendors/chart.js/Chart.min.js"></script>
    <script src="~/admin/vendors/datatables.net/jquery.dataTables.js"></script>
    <script src="~/admin/vendors/datatables.net-bs4/dataTables.bootstrap4.js"></script>
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="~/admin/js/off-canvas.js"></script>
    <script src="~/admin/js/hoverable-collapse.js"></script>
    <script asp-append-version="true" src="~/admin/js/template.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src="~/admin/js/dashboard.js"></script>
    <script src="~/admin/js/data-table.js"></script>
    <script src="~/admin/js/jquery.dataTables.js"></script>
    <script src="~/admin/js/dataTables.bootstrap4.js"></script>
    <!-- End custom js for this page-->
    <script src="~/admin/js/jquery.cookie.js" type="text/javascript"></script>
    @RenderSection("Scripts", false)
</body>

</html>
