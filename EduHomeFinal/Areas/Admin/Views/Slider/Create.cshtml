@model Slider


<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for=""> Şəkil</label>
        <input asp-for="@Model.Photo" class="form-control">
        <span asp-validation-for="@Model.Photo" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for=""> Arxa Fon Şəkli</label>
        <input asp-for="@Model.Photo2" class="form-control">
        <span asp-validation-for="@Model.Photo2" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="">Başlıq </label>
        <input asp-for="@Model.Title" class="form-control">
        <span asp-validation-for="@Model.Title" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="">Başlıq2 </label>
        <input asp-for="@Model.SubTitle" class="form-control">
        <span asp-validation-for="@Model.SubTitle" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label for="">Təsvir </label>
        <input asp-for="@Model.Description" class="form-control">
        <span asp-validation-for="@Model.Description" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-primary">Yarat</button>
</form>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.2/jquery.validate.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.11/jquery.validate.unobtrusive.min.js"></script>
